<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans.xsd
                           http://code.alibabatech.com/schema/dubbo
                           http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="lks-provider" />

    <dubbo:registry protocol="zookeeper" address="zookeeper://124.221.218.66:2181" />

    <dubbo:protocol name="dubbo" port="20881" />

    <dubbo:service interface="org.example.api.service.GoodService" ref="goodService" group="A" version="1.0.0" />

    <bean id="goodService" class="org.example.service.impl.GoodServiceImpl"/>
</beans>
